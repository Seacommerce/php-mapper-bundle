{
  "name": "seacommerce/mapper-bundle",
  "type": "bundle",
  "description": "Symfony bundle for seacommerce/mapper",
  "keywords": [
    "seacommerce"
  ],
  "homepage": "https://seacommerce.nl",
  "license": "MIT",
  "authors": [
    {
      "name": "Sil Moelker",
      "email": "sil@seacommerce.nl"
    },
    {
      "name": "Niels Steenbakker",
      "email": "niels@seacommerce.nl"
    },
    {
      "name": "Seacommerce",
      "homepage": "https://seacommerce.nl",
      "email": "info@seacommerce.nl"
    }
  ],
  "repositories": [
  ],
  "require": {
    "php": ">=7.1",
    "symfony/http-kernel": "^4.2",
    "symfony/yaml": "^4.2",
    "symfony/config": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "seacommerce/mapper": "^0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "doctrine/orm": "^2.6",
    "symfony/orm-pack": "^1.0",
    "symfony/filesystem": "^4.2",
    "ocramius/proxy-manager": "2.1.1",
    "symfony/monolog-bundle": "^3.1",
    "monolog/monolog": "^1.23"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Seacommerce\\MapperBundle\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seacommerce\\MapperBundle\\Test\\": "./tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
